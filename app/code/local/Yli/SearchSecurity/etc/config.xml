<?xml version="1.0"?>
<config>
      <modules>
        <Yli_SearchSecurity>
          <version>0.1.0</version>
        </Yli_SearchSecurity>
      </modules>
      <global>
          <helpers>
            <searchsecurity>
              <class>Yli_SearchSecurity_Helper</class>
            </searchsecurity>
          </helpers>
          <models>
             <searchsecurity>
                <class>Yli_SearchSecurity_Model</class>
             </searchsecurity>
          </models>
      </global>
      <frontend>
		<routers>
			<catalogsearch>
				<args>
					<modules>
						<searchsecurity before="Mage_CatalogSearch">Yli_SearchSecurity</searchsecurity>
					</modules>
				</args>
			</catalogsearch>
		</routers>
	</frontend>
</config>